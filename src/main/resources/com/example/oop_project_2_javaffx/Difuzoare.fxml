<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER" prefHeight="428.0" prefWidth="376.0" spacing="20.0" xmlns="http://javafx.com/javafx/23" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.oop_project_2_javaffx.Difuzoare_Controller">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <children>
      <Label fx:id="label_cam_info" text="Camera: ">
         <font>
            <Font name="System Italic" size="12.0" />
         </font>
      </Label>
      <Label prefHeight="20.0" prefWidth="75.0" text="Difuzoare: ">
         <font>
            <Font name="System Bold Italic" size="14.0" />
         </font>
      </Label>
      <Button fx:id="butt_dif_1" mnemonicParsing="false" onAction="#on_dif_cam_1" text="Difuzor 1" />
      <Button fx:id="butt_dif_2" disable="true" mnemonicParsing="false" onAction="#on_dif_cam_2" text="Difuzor 2" />
      <Button fx:id="butt_dif_3" disable="true" mnemonicParsing="false" onAction="#on_dif_cam_3" text="Difuzor 3" />
      <Button fx:id="butt_dif_4" disable="true" mnemonicParsing="false" onAction="#on_dif_cam_4" text="Difuzor 4" />
      <Button fx:id="butt_dif_5" disable="true" mnemonicParsing="false" onAction="#on_dif_cam_5" text="Difuzor 5" />
      <Button fx:id="butt_back_dif" onAction="#on_back" mnemonicParsing="false" prefHeight="34.0" prefWidth="85.0" text="&lt; Back" />
   </children>
</VBox>
